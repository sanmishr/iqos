<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Flex Container"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.container.v1.editor]"
    helpPath="https://css-tricks.com/snippets/css/a-guide-to-flexbox/"
    trackingFeature="core-components:container:v1">
    <content
        granite:class="cmp-container__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <flex
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Flex"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <flexDirection
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Direction"
                                                name="./flexDirection">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <row
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Row"
                                                        value="row"/>
                                                    <rowReverse
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Row Reverse"
                                                        value="row-reverse"/>
                                                    <column
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Column"
                                                        value="column"/>
                                                    <columnReverse
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Column Reverse"
                                                        value="column-reverse"/>
                                                </items>
                                            </flexDirection>
                                            <flexWrap
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Wrap"
                                                name="./flexWrap">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <nowrap
                                                        jcr:primaryType="nt:unstructured"
                                                        text="NoWrap"
                                                        value="nowrap "/>
                                                    <wrap
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Wrap"
                                                        value="wrap"/>
                                                    <wrapReverse
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Wrap Reverse"
                                                        value="wrapReverse"/>
                                                </items>
                                            </flexWrap>
                                            <justifyContent
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Justify Content"
                                                name="./justifyContent">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <flexStart
                                                        jcr:primaryType="nt:unstructured"
                                                        text="flex-start"
                                                        value="flex-start"/>
                                                    <flexEnd
                                                        jcr:primaryType="nt:unstructured"
                                                        text="flex-end"
                                                        value="flex-end"/>
                                                    <center
                                                        jcr:primaryType="nt:unstructured"
                                                        text="center"
                                                        value="center"/>
                                                    <spaceBetween
                                                        jcr:primaryType="nt:unstructured"
                                                        text="space-between"
                                                        value="space-between"/>
                                                    <spaceAround
                                                        jcr:primaryType="nt:unstructured"
                                                        text="space-around"
                                                        value="space-around"/>
                                                    <spaceEvenly
                                                        jcr:primaryType="nt:unstructured"
                                                        text="space-evenly"
                                                        value="space-evenly"/>
                                                </items>
                                            </justifyContent>
                                            <alignContent
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Align Content"
                                                name="./alignContent">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <flexStart
                                                        jcr:primaryType="nt:unstructured"
                                                        text="flex-start"
                                                        value="flex-start"/>
                                                    <flexEnd
                                                        jcr:primaryType="nt:unstructured"
                                                        text="flex-end"
                                                        value="flex-end"/>
                                                    <center
                                                        jcr:primaryType="nt:unstructured"
                                                        text="center"
                                                        value="center"/>
                                                    <spaceBetween
                                                        jcr:primaryType="nt:unstructured"
                                                        text="space-between"
                                                        value="space-between"/>
                                                    <spaceAround
                                                        jcr:primaryType="nt:unstructured"
                                                        text="space-around"
                                                        value="space-around"/>
                                                    <stretch
                                                        jcr:primaryType="nt:unstructured"
                                                        text="stretch"
                                                        value="stretch"/>
                                                </items>
                                            </alignContent>
                                            <alignItems
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Align Items"
                                                name="./alignItems">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <flexStart
                                                        jcr:primaryType="nt:unstructured"
                                                        text="flex-start"
                                                        value="flex-start"/>
                                                    <flexEnd
                                                        jcr:primaryType="nt:unstructured"
                                                        text="flex-end"
                                                        value="flex-end"/>
                                                    <center
                                                        jcr:primaryType="nt:unstructured"
                                                        text="center"
                                                        value="center"/>
                                                    <baseline
                                                        jcr:primaryType="nt:unstructured"
                                                        text="baseline"
                                                        value="baseline"/>
                                                    <stretch
                                                        jcr:primaryType="nt:unstructured"
                                                        text="stretch"
                                                        value="stretch"/>
                                                </items>
                                            </alignItems>
                                            <flexGrow
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldLabel="Flex Grow"
                                                max="{Long}5"
                                                min="{Long}1"
                                                name="./flexGrow"
                                                step="{Double}1.0"/>
                                            <flexShrink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldLabel="Flex Shrink"
                                                max="{Long}5"
                                                min="{Long}1"
                                                name="./flexShrink"
                                                step="{Double}1.0"/>
                                            <paddingleft
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="padding for container "
                                                fieldLabel="Padding Left"
                                                name="./paddingLeft"
                                                type="editable">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <none
                                                        jcr:primaryType="nt:unstructured"
                                                        text="None"
                                                        value="none"/>
                                                    <small
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Small"
                                                        value="padding-left-sm"/>
                                                    <medium
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Medium"
                                                        value="padding-left-md"/>
                                                    <large
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Large"
                                                        value="padding-left-lg"/>
                                                </items>
                                            </paddingleft>
                                            <paddingright
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="padding for container "
                                                fieldLabel="Padding Right"
                                                name="./paddingRight"
                                                type="editable">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <none
                                                        jcr:primaryType="nt:unstructured"
                                                        text="None"
                                                        value="none"/>
                                                    <small
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Small"
                                                        value="padding-right-sm"/>
                                                    <medium
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Medium"
                                                        value="padding-right-md"/>
                                                    <large
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Large"
                                                        value="padding-right-lg"/>
                                                </items>
                                            </paddingright>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </flex>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
