    "pako": "^2.1.0",
    "react": "^16.14.0",
    "react-app-polyfill": "^1.0.5",
    "react-dom": "^16.14.0",
    "react-fast-compare": "^3.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-universal-component": "^4.5.0",
    "regenerator-runtime": "^0.13.11",
    "sanitize-html": "^2.7.3",
    "sass": "^1.56.2"
  },
  "devDependencies": {
    "@adobe/aio-cli": "^9.1.1",
